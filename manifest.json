{
  "manifest_version": 3,
  "name": "nhentai/x linker",
  "version": "1.0",
  "description": "A Chrome extension that turns selected numbers/x account into nhentai/x link.",
  "permissions": ["contextMenus", "tabs"],
  "background": {
      "service_worker": "main.js"
  },
  "host_permissions": ["https://nhentai.net/*","https://x.com/*"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
